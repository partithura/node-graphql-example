{
  "name": "node-graphql-example",
  "license": "PROPRIETARY",
  "packageManager": "yarn@3.5.0",
  "type": "commonjs",
  "exports": "./dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "build": "yarn run generate && tsc",
    "generate": "graphql-codegen --config codegen.ts",
    "knex": "ts-node -r dotenv/config ./node_modules/.bin/knex --knexfile ./src/infra/db/knexfile.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@as-integrations/fastify": "^1.3.2",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "http-terminator": "^3.2.0",
    "knex": "^2.4.2",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "tslib": "^2.5.0",
    "ulidx": "^0.5.0"
  },
  "devDependencies": {
    "@eddeee888/gcg-typescript-resolver-files": "^0.3.0",
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-resolvers": "^3.1.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
